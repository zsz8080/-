/* pages/home/home.wxss */
.wrap{
    width:100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    position: relative;
    box-sizing: border-box;
}
.head{
    width:100%;
    height: 500rpx;
    background: blue;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
}
.btn{
    background: royalblue;
    height: 80rpx;
    color:#fff;
    width:90%;
    border-radius: 50rpx;
    position: absolute;
    top:38%;
    left:5%;
    text-align: center;
    line-height: 80rpx;
}
.list{
    flex:1;
    display: flex;
    background: #eee;
    flex-wrap: wrap;
    justify-content: space-around;
    padding: 50rpx 30rpx;
}
.dl{
    width:30%;
    height: 30%;
    display: flex;
    padding: 30rpx 30rpx;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background: #fff;
}
image{
    width:150rpx;
    height: 150rpx;
}