/* pages/room/room.wxss */
.wrap{
    width:100%;
    height: 100%;
    background: #eee;
    
}
image{
    width:100rpx;
    height:100rpx;
}
.list{
    width:100%;
    height: 100%;
    display: flex;
    flex-wrap: wrap;
    padding: 10rpx;
    align-content: space-between;
    align-items: center;
}
.dl{
    width:200rpx;
    height: 300rpx;
    display: flex;
    padding: 5rpx;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background: #fff;
    margin:10rpx;
}
.reduce{
    display: flex;
}
.dl .btn{
    width:50rpx;
    height: 50rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 100%;
    background: #eee;
}
.bot{
    width:100%;
    height: 300rpx;
    background: #fff;
    display: flex;
    position: fixed;
    bottom: 0;
    left:0;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}