/* pages/list/list.wxss */
page{
    width: 100%;
    height: 100%;
}
.wrap{
    width:100%;
    height: 100%;
    padding: 30rpx;
}
.dl{
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx;
}
image{
    width:100rpx;
    height: 100rpx;
}
.dl view{
    flex:1;
    display: flex;
    flex-direction: column;
}
.dl view text{
    flex:1;
    display: flex;
    flex-direction: column;
    justify-content: center;
}